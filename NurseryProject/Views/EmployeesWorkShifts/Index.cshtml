@model IEnumerable<NurseryProject.Dtos.EmployeesWorkShifts.EmployeesWorkShiftsDto>
@using NurseryProject.Utilities
@{
    ViewBag.Title = "ادارة فترات الموظفين ";
}
<div class="card">
    <div class="card-header">

        <div class="row">
            <h2 class="col">  </h2>
            <p class="col-auto">
                @Url.Create()
            </p>
        </div>
    </div>
    <div class="card-body">
        <table id="datatable" class="table table-striped dt-responsive  nowrap w-100">
            <thead class="bg-soft-primary">
                <tr>
                    @*<th>
            @Html.DisplayName("مكان الدراسة")
        </th>*@
                    <th>
                        @Html.DisplayName("الكود")
                    </th>
                    <th>
                        @Html.DisplayName("الاسم")
                    </th>
                    <th>
                        @Html.DisplayName("الفترة")
                    </th>
                    <th>
                        @Html.DisplayName("الادارة")
                    </th>

                    <th>
                        @Html.DisplayName("عمليات")
                    </th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                <tr>
                    @*<td>
            @Html.DisplayFor(modelItem => item.StudyPlaceName)
        </td>*@
                    @*<td>
            @Html.DisplayFor(modelItem => item.StudyYearName)
        </td>*@
                    <td>
                        @Html.DisplayFor(modelItem => item.Code)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.EmployeeName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.WorkShiftName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.DepartmentName)
                    </td>
                    <td>
                        @Url.Edit(routeValues: new { id = item.Id })
                        @Url.Delete(routeValues: new { id = item.Id })
                    </td>
                </tr>
                }
            </tbody>
        </table>

    </div>
</div>